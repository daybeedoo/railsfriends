<tr id="<%= dom_id friend %>">
  <th scope="row">
    <%= friend.id %>
  </th>
  <td>
    <%= friend.first_name %>
  </td>

  <td>
    <%= friend.last_name %>
  </td>

  <td>
    <%= friend.email %>
  </td>

  <td>
    <%= friend.phone %>
  </td>

  <td>
    <%= friend.twitter %>
  </td>

  <td>
    <%= friend.user_id %>
  </td>

  <td>
    <%= link_to "Show", friend, class:"btn btn-secondary" %>
  </td>

  <td>
    <%= link_to "Edit", edit_friend_path(friend), class:"btn btn-secondary" %>
  </td>

  <td>
    <%= button_to "Destroy", friend, method: :delete, data: {confirm: 'Are you sure?'}, class:"btn btn-secondary" %>
  </td>

</tr>